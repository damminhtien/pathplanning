{
  "include": [
    "pathplanning/core",
    "pathplanning/env",
    "pathplanning/api.py",
    "pathplanning/viz",
    "pathplanning/planners/sampling/rrt.py",
    "pathplanning/planners/sampling/rrt_star.py",
    "pathplanning/viz/sampling3d_plotting.py",
    "examples/worlds/demo_3d_world.py"
  ],
  "exclude": [
    "tests",
    "dist",
    "build",
    ".venv",
    "pathplanning.egg-info",
    "pathplanningv2.egg-info"
  ],
  "typeCheckingMode": "basic",
  "strict": [
    "pathplanning/core",
    "pathplanning/env",
    "pathplanning/planners/sampling/rrt.py",
    "pathplanning/planners/sampling/rrt_star.py",
    "pathplanning/api.py"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": "warning"
}
